! Cursor Theme
Xcursor.theme: Vanilla-DMZ

! URxvt General
URxvt.modifier: mod1
URxvt.depth: 32
URxvt.background: [80]#1c2023
URxvt.scrollBar: false
URxvt.internalBorder: 10
URxvt.letterSpace: -1
UXTerm*bellIsUrgent: true

URxvt.colorUL: #4682B4
URxvt*cutchars: `"()'*<>[]{|}

URxvt.cursorBlink: true
URxvt.cursorUnderline: true

URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true

URxvt.font: xft:EnvyCodeR:size=14:antialias=true
Xft.autohint:   0
Xft.dpi:        96
Xft.hinting:    1
Xft.hintstyle:  hintslight
Xft.lcdfilter:  lcddefault
Xft.rgba:       rgb

! Extensions
URxvt.perl-ext-common: default,resize-font,matcher,keyboard-select,selection-to-clipboard

URxvt.url-launcher: /usr/bin/firefox -new-tab
URxvt.matcher.button: 1

URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search

URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger: C-Up

! Colors - colors everywhere!
rofi.color-enabled: true
rofi.color-window: #273238, #273238, #1e2529
rofi.color-normal: #273238, #c1c1c1, #273238, #394249, #ffffff
rofi.color-active: #273238, #80cbc4, #273238, #394249, #80cbc4
rofi.color-urgent: #273238, #ff1844, #273238, #394249, #ff1844


! special
*.foreground:   #93a1a1
*.background:   #002b36
*.cursorColor:  #93a1a1

